<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Main13Activity">

    <com.google.android.material.appbar.AppBarLayout
        android:id="@+id/appbar5"

        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:theme="@style/AppTheme.AppBarOverlay"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <com.google.android.material.tabs.TabLayout
            android:id="@+id/tabs5"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            app:tabIndicatorColor="#E91E63">

            <com.google.android.material.tabs.TabItem
                android:id="@+id/tabItem51"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/ad25" />

            <com.google.android.material.tabs.TabItem
                android:id="@+id/tabItem52"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/ad26" />


        </com.google.android.material.tabs.TabLayout>

    </com.google.android.material.appbar.AppBarLayout>

    <androidx.viewpager.widget.ViewPager
        android:id="@+id/container5"
        android:layout_width="411dp"
        android:layout_height="553dp"
        app:layout_behavior="@string/appbar_scrolling_view_behavior"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/appbar5" />

</androidx.constraintlayout.widget.ConstraintLayout>